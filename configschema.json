{
    "$schema": "http://json-schema.org/draft-07/schema",
    "type": "object",
    "additionalProperties": false,
    "properties": {
        "livesplit": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "address": {
                    "type": "string",
                    "default": "127.0.0.1:16834"
                },
                "tick": {
                    "type": "number"
                }
            },
            "required": ["address"]
        }
    },
    "required": [
        "livesplit"
    ]
}